<configuration debug="false" scan="true" scanPeriod="5 seconds">
  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n</pattern>
    </encoder>
  </appender>

  <logger name="com.github.kaklakariada.html.HtmlElement" level="debug" />
  <logger name="com.github.kaklakariada.fritzbox.report.convert.DataExtractor" level="debug" />
  
  <root level="debug">
    <appender-ref ref="STDOUT" />
  </root>
</configuration>